import java.util.*;
public class Solution {
    public static void main(String[] args) {
        Scanner s = new Scanner(System.in);
        int a = s.nextInt();
        for(int row=1;row<=a;row++){
            for(int j=1;j<=row;j++){
                System.out.print("*");
            }System.out.println();
        }
    }
}
