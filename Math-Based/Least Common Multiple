import java.util.*;
public class Solution {
    public static void main(String[] args) {
        Scanner s = new Scanner(System.in);
        int a = s.nextInt();
        int b = s.nextInt();
        int c =a*b;
        int gcd = 1;
        for(int i=1;i<=b ;i++){
            if(a%i==0 && b%i==0){
                gcd=i;
            }
        }
        int lcm = c/gcd;
        System.out.println(lcm);
    }
}
